<odoo>
    <!-- Rule untuk Semua Pengguna (Akses Lihat Semua Travel Package) -->
    <record id="travel_package_all_user_rule" model="ir.rule">
        <field name="name">Travel Package / All User Access</field>
        <field name="model_id" ref="model_travel_package"/>
        <field name="domain_force">[(1, '=', 1)]</field> <!-- Akses ke Semua Data -->
        <field name="groups" eval="[]"/> <!-- Tidak ada pembatasan berdasarkan grup -->
    </record>
</odoo>
